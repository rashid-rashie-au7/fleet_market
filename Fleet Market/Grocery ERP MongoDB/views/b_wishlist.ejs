<!DOCTYPE HTML>
<html lang="en">
<head>
<meta charset="utf-8">
<meta http-equiv="pragma" content="no-cache" />
<meta http-equiv="cache-control" content="max-age=604800" />
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

<title>Fleet Market</title>
<!-- jQuery -->
<script src="/js/jquery-2.0.0.min.js" type="text/javascript"></script>
<!-- Bootstrap4 files-->
<script src="/js/bootstrap.bundle.min.js" type="text/javascript"></script>
<link href="/css/bootstrap.css" rel="stylesheet" type="text/css"/>
<!-- Font awesome 5 -->
<link href="/fonts/fontawesome/css/all.min.css" type="text/css" rel="stylesheet">
<!-- custom style -->
<link href="/css/ui.css" rel="stylesheet" type="text/css"/>
<link href="/css/responsive.css" rel="stylesheet" type="text/css" />
<!-- custom javascript -->
<script src="/js/script.js" type="text/javascript"></script>
</head>
<body>
<!-- ========================= SECTION HEADER ========================= -->
<header class="section-header">
	<section class="header-main border-bottom">
	  	<div class="container">
			<div class="row align-items-center">
				<div class="col-xl-2 col-lg-3 col-md-12">
					<a> <h4 class="title-section text-uppercase">Fleet Market </h4></a> 
				</div>
				<div class="col-xl-6 col-lg-5 col-md-6">
					<form action="#" class="search-header">
						<div class="input-group w-100">
							<input type="text" class="form-control" placeholder="Search">       
							<div class="input-group-append">
								<button class="btn btn-primary" type="submit">
									<i class="fa fa-search"></i> Search
								</button>
							</div>
						</div>
					</form> 
				</div> 
				<div class="col-xl-4 col-lg-4 col-md-6">
					<div class="widgets-wrap float-md-right">
						<div class="widget-header mr-3">
							<p style="font-size: 18px; font-style: oblique ;"> Hi,<%= firstname%></p>
						</div>
						<div class="widget-header mr-3">
							<a href="/homebuyer" class="widget-view">
								<div class="icon-area">
									<i class="fa fa-home" style="color: darkorange;"></i>
								</div>
								<small class="text"> Home </small>
							</a>
						</div>
						<div class="widget-header">
							<a href="/mycart" class="widget-view">
								<div class="icon-area">
									<i class="fa fa-shopping-cart" style="color: darkorange;"></i>
									
								</div>
								<small class="text"> Cart </small>
							</a>
						</div>
						<div class="widget-header mr-3">
							<a class="nav-link dropdown-toggle" data-toggle="dropdown" href="#"> <i class="fa fa-user" style="color: darkorange;"></i> Accounts </a>
							<div class="dropdown-menu">
								<a class="dropdown-item" href="/myprofile"><i class="fa fa-user-circle"></i> My Profile</a>
								<a class="dropdown-item" href="/myorders"><i class="fa fa-store"></i> My Orders</a>
								<a class="dropdown-item" href="/mywishlist"><i class="fa fa-heart"></i>  Wishlist</a>
								<a class="dropdown-item" href="/logout"><i class="fa  fa-lock"></i>  Logout</a>
							  </div>
							</a>
						</div>
					</div> 
				</div> 
			</div>
				 
	  	</div> 
	</section>
	<nav class="navbar navbar-main navbar-expand-lg border-bottom">
	  	<div class="container">
			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main_nav" aria-controls="main_nav" aria-expanded="false" aria-label="Toggle navigation">
			<span class="navbar-toggler-icon"></span>
			</button>
			<div class="collapse navbar-collapse" id="main_nav">
				<ul class="navbar-nav">
					<li class="nav-item">
						<a class="nav-link" href="/category/?cat=Grocery">Grocery</a>
					</li>
					<li class="nav-item">
						<a class="nav-link" href="/category/?cat=Vegetables & Fruits">Vegetables & Fruits</a>
					</li>
					<li class="nav-item">
					   <a class="nav-link" href="/category/?cat=Kitchen Accessories">Kitchen Accessories</a>
					</li>
					<li class="nav-item">
					   <a class="nav-link" href="/category/?cat=Essentials">Essentials</a>
					</li>
					<li class="nav-item">
					   <a class="nav-link" href="/category/?cat=Beverages">Beverages</a>
					</li>
				</ul>
			</div> 
	  	</div> 
	</nav>
</header> 
<!-- ========================= SECTION HEADER END========================= -->
<!-- ========================= SECTION CONTENT ========================= -->
<section class="section-content padding-y">
	<div class="container">
		<% if( prdts.length != 0){ %>
		<div class="row">
			<main class="col-md-10">
				
				<header class="section-heading heading-line">
					<h4 class="title-section text-uppercase"><i class="fa fa-heart"></i> My Wishlist</h4>
				</header>
				
				<header class="mb-3">
					<div class="form-inline">
						<strong class="mr-md-auto"><%= prdts.length%> items found </strong>
					</div>
				</header>
				<% prdts.forEach(function(prdt) {%>
				<article class="card card-product-list">
					
					<div class="row no-gutters">
						<aside class="col-md-3">
							<a href="/detailproduct/?id=<%= prdt.prdtid %>" class="img-wrap">
								<img src="images/items/<%=prdt.imgpath%>">
							</a>
						</aside>
						<div class="col-md-6">
							<div class="info-main">
								<a href="/detailproduct/?id=<%= prdt.prdtid %>" class="h5 title"><%= prdt.prdtname %> </p>
							</div>
							<div>
								<small class="text-muted"><%= prdt.descn%></small>
							</div>
							<%  var price = (prdt.price).toFixed(2) , off =prdt.offer, offer  %>
							<% offer = price - ((price* off)/100) %>
							<div class="price mt-1"> <p style="font-size: 24px; font-weight: 500; display: inline; font-family:Cochin">â‚¹ <%=Math.round(offer*2)/2 - 0.01 %> </p>  
								
								<span class="badge badge-danger" style="margin-left: 10px; margin-bottom:15px;"><%=prdt.offer %> % Off </span>
							</div>
						</div> 

						<aside class="col-md-3">
							<div class="info-aside">
								<div class="price-wrap">
									<span class="h5 price"><%= (prdt.price).toFixed(2) %><small class="text-muted">/per item</small></span>	
								</div> <!-- price-wrap.// -->
								<small class="text" style="color: green;">Free delivery in 2-3 days</small>
								<div class="h6"><%= prdt.brand%></div>
								<div class="h6"><%= prdt.catgy%></div>
								<div class="mt-3">
									<a href="/removewishlist/?id=<%=prdt.prdtid%>" class="btn btn-outline-primary"> <i class="fa fa-trash"></i> Remove Item </a>
								</div>
							</div>
						</aside> 
					</div> 	
				</article>
				<% }); %>
			</main> 
		</div>
		<% } else { %>
			<div style="text-align: center; margin-top: 40px;margin-bottom: 40px;">
				<img src="images/emptyicons/empty_wishlist.png" style="justify-content:center;">	   
			</div>  
		<% } %>
	</div> 
</section>
<!-- ========================= SECTION CONTENT END// ========================= -->

<!-- ========================= FOOTER ========================= -->
<footer class="section-footer bg-secondary">
	<div class="container">	
		<section class="footer-bottom text-center">
			<p class="text-muted"> Grocery ERP Managemnet System </p>
		</section>
	</div>
</footer>
<!-- ========================= FOOTER END // ========================= -->
</body>
</html>